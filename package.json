{
    "name": "intelligent-stock-recommendation",
    "version": "1.0.0",
    "description": "Intelligent Stock Trading Recommendation System",
    "private": true,
    "workspaces": [
        "packages/*",
        "apps/*"
    ],
    "scripts": {
        "build": "turbo run build",
        "dev": "turbo run dev",
        "lint": "turbo run lint",
        "test": "turbo run test",
        "type-check": "turbo run type-check",
        "clean": "turbo run clean",
        "deploy": "npm run build && cdk deploy --all",
        "deploy:dev": "npm run build && cdk deploy --all --profile dev",
        "deploy:prod": "npm run build && cdk deploy --all --profile prod",
        "synth": "cdk synth",
        "diff": "cdk diff",
        "bootstrap": "cdk bootstrap"
    },
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^6.0.0",
        "@typescript-eslint/parser": "^6.0.0",
        "eslint": "^8.0.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.0.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.0.0",
        "turbo": "^1.10.0",
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "aws-cdk": "^2.100.0",
        "aws-cdk-lib": "^2.100.0",
        "constructs": "^10.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}